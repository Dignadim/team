<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<!-- itemList의 빈(기본생성자) -->
	<bean id="itemList" class="project.item.ItemList"/>
	<!-- itemCommentList의 빈(기본생성자) -->
	<bean id="itemCommentList" class="project.item.ItemCommentList"/>
	
	<!-- 아이템 평점의 빈 -->
	<bean id="itemAvgVO" class="project.item.ItemAvgVO"/>
	
	
	<!-- 회원정보의 빈  -->
	<bean id="memberVO" class="project.member.MemberVO"/>
	<bean id="memberList" class="project.member.MemberList"/>
	
	<!-- 자유게시판의 빈 -->
	<bean id="freeList" class="project.board.free.FreeboardList"/>
	<!-- FreeboardCommentList의 빈 -->
	<bean id="freeboardCommentList" class="project.board.free.FreeboardCommentList"/>

	<!-- 행사게시판의 빈 -->
	<bean id="eventList" class="project.board.event.EventboardList"/>
	<!-- EventboardCommentList의 빈 -->
	<bean id="eventboardCommentList" class="project.board.event.EventboardCommentList"/>
	<!-- 스케쥴 vo -->
	<bean id="scheduleVO" class="project.util.calendar.ScheduleVO"/>
	
	
</beans>
